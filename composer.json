{
    "name": "steffjenl/laravel-azure-blob-storage",
    "description": "Microsoft Azure Blob Storage integration for Laravel's Storage API",
    "type": "library",
    "keywords": ["storage","laravel","azure"],
    "require": {
        "league/flysystem-azure-blob-storage": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "psy/psysh": "^0.8.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "St√®phan Eizinga",
            "email": "stephan@monkeysoft.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "SteffjeNL\\LaravelAzureBlobStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SteffjeNL\\LaravelAzureBlobStorage\\AzureBlobStorageServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:steffjenl/flysystem-azure-blob-storage.git"
        }
    ]
}
